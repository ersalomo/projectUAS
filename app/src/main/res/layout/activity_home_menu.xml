<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.admin.HomeMenuActivity">

    <androidx.cardview.widget.CardView
            android:layout_width="384dp"
            android:layout_height="434dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.384"
            app:layout_constraintVertical_bias="0.377" android:layout_marginStart="1dp">
        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="414dp">
            <ImageView
                    android:layout_width="148dp"
                    android:layout_height="158dp"
                    app:srcCompat="@drawable/listdogsimage"
                    android:id="@+id/ivListDogs"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="112dp"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginTop="52dp"/>
            <ImageView
                    android:id="@+id/iv_makananDog"
                    android:src="@drawable/img"
                    android:layout_width="147dp"
                    android:layout_height="149dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginTop="232dp"
                    app:layout_constraintStart_toEndOf="@+id/ivMainanDog" app:layout_constraintHorizontal_bias="0.659"/>
            <ImageView
                    android:id="@+id/ivMainanDog"
                    android:src="@drawable/imanananjing"
                    android:layout_width="148dp"
                    android:layout_height="149dp"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="16dp"
                    app:layout_constraintTop_toBottomOf="@+id/ivListDogs" android:layout_marginTop="20dp"
                    app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.0"/>
            <ImageView
                    android:id="@+id/iv_about"
                    android:src="@drawable/about"
                    android:layout_width="77dp"
                    android:layout_height="52dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_marginStart="4dp" android:layout_marginBottom="12dp"
                    app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.494"
                    app:layout_constraintStart_toStartOf="parent"/>


        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>